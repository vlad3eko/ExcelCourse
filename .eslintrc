{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "padded-blocks": "off",
    "arrow-parens": "off",
    "no-tabs": "off",
    "indent": "off",
    "quotes": "off",
    "eol-last": "off",
    "require-jsdoc": "off",
    "no-multiple-empty-lines": "off",
    "no-unused-vars": "off",
    "no-debugger": "off",
    "operator-linebreak": "off",
    "object-curly-spacing": "off",
    "no-trailing-spaces": "off",
    "max-len": "off",
    "space-before-function-paren": "off",
  },
  "extends": ["eslint:recommended", "google"]
}

